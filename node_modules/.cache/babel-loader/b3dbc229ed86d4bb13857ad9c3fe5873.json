H;;;;;;AAIL,eAAeZ,WAAf","sourcesContent":["//import axios from axios\n\nconst url = 'http://localhost:5000/api/posts/';\n\nclass postService {\n    // GET POST\n    static getPosts() {\n        return new Promise(async (resolve, reject) => {\n            try {\n                const res = await axios.get(url);\n                const data = res.data;\n                resolve(\n                    data.map(name => ({  // check this method in function to the data base\n                        ...name,\n                        ...email\n                    }))\n                );\n            } catch(err) {\n                reject(err);\n            }\n        });\n    }\n\n    // CREATE POST\n    static insertPost(name, email) {\n        return axios.post(url, {\n            name,\n            email\n        });\n    }\n\n    // DELETE POST\n    static deletePost(id) {\n        return axios.delete(`${url}${id}`);\n    }\n\n}\n\nexport default postService;"]}]}